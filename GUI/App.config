<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
      <section name="paths" type="System.Configuration.AppSettingsSection" />
    </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.1" />
    </startup>
    <paths>
        <add key="./../../../Model/bin/Debug" />
        <!-- <add key="./../../../DatabaseLogging/bin/Debug" /> -->
        <add key="./../../../Logging/bin/Debug" />
        <add key="./../../../DatabaseSerialization/bin/Debug" />
        <!-- <add key="./../../../Serialization/bin/Debug" /> -->
        <add key="./../../../GUI/bin/Debug" />
    </paths>
    <appSettings>
        <add key="logDb" value="./../../../Databases/TpaLoggingDatabase.mdf" />
        <add key="modelDb" value="./../../../Databases/TpaModelDatabase.mdf" />
    </appSettings>
    <connectionStrings>
        <add name="FileDatabase" connectionString="Data Source=(LocalDB)\MSSQLLocalDB;AttachDbFilename=|DataDirectory|;Integrated Security=True;Connect Timeout=30;Context Connection=False" providerName="System.Data.SqlClient"/>
    </connectionStrings>
</configuration>